#!/usr/bin/env bash
# Bash script to display "Best School" 10 times using an until loop

# Initialize a counter
counter=0

# Until loop to display "Best School" as long as the counter is less than 10
until [ $counter -ge 10 ]; do
    # Display "Best School"
    echo "Best School"

    # Increment the counter
    ((counter++))
done
